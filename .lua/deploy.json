{
  "skills": [
    {
      "name": "grocery-ordering-skill",
      "description": "Tools for grocery ordering from UK stores (Sainsbury's, Tesco, ASDA, Waitrose). Handles item parsing, price comparison, basket management, and checkout.",
      "context": "Use these tools when users want to order groceries. The flow should be:\n    1. Parse items from user input (items can be separated by commas, spaces, or periods)\n    2. Show prices from available stores (Sainsbury's, Tesco, ASDA, Waitrose)\n    3. Allow user to compare stores and choose cheapest or fastest delivery\n    4. Allow user to add or remove items from basket at any time\n    5. When user says \"ok\" or confirms, ask for delivery address\n    6. Process checkout and show thank you message\n    \n    Be conversational and helpful. Always show item prices clearly. Let users modify their basket before final checkout.",
      "tools": [
        {
          "name": "parse_grocery_items",
          "description": "Parse a list of grocery items from user input. Items can be separated by commas, spaces, or periods.",
          "inputSchema": {
            "type": "object",
            "properties": {
              "userInput": {
                "type": "string",
                "description": "The user's input containing grocery items separated by commas, spaces, or periods"
              }
            },
            "required": [
              "userInput"
            ],
            "additionalProperties": false
          },
          "execute": ""
        },
        {
          "name": "get_store_prices",
          "description": "Get prices for grocery items from a specific UK store (Sainsburys, Tesco, ASDA, or Waitrose). Returns item prices, total cost, and delivery time.",
          "inputSchema": {
            "type": "object",
            "properties": {
              "store": {
                "type": "string",
                "enum": [
                  "sainsbury",
                  "tesco",
                  "asda",
                  "waitrose"
                ],
                "description": "The store name (lowercase)"
              },
              "items": {
                "type": "array",
                "items": {
                  "type": "string"
                },
                "description": "List of grocery items to get prices for"
              }
            },
            "required": [
              "store",
              "items"
            ],
            "additionalProperties": false
          },
          "execute": ""
        },
        {
          "name": "compare_stores",
          "description": "Compare prices and delivery times across all UK stores (Sainsburys, Tesco, ASDA, Waitrose) for given items. Returns comparison with cheapest and fastest options.",
          "inputSchema": {
            "type": "object",
            "properties": {
              "items": {
                "type": "array",
                "items": {
                  "type": "string"
                },
                "description": "List of grocery items to compare"
              },
              "preference": {
                "type": "string",
                "enum": [
                  "cheapest",
                  "fastest",
                  "both"
                ],
                "description": "Whether to find cheapest, fastest, or both (default: both)"
              }
            },
            "required": [
              "items"
            ],
            "additionalProperties": false
          },
          "execute": ""
        },
        {
          "name": "manage_basket",
          "description": "Add items to or remove items from the grocery basket. Returns updated basket with item count and total.",
          "inputSchema": {
            "type": "object",
            "properties": {
              "currentItems": {
                "type": "array",
                "items": {
                  "type": "string"
                },
                "description": "Current list of items in the basket"
              },
              "action": {
                "type": "string",
                "enum": [
                  "add",
                  "remove"
                ],
                "description": "Whether to add or remove items"
              },
              "items": {
                "type": "array",
                "items": {
                  "type": "string"
                },
                "description": "Items to add or remove from the basket"
              }
            },
            "required": [
              "currentItems",
              "action",
              "items"
            ],
            "additionalProperties": false
          },
          "execute": ""
        },
        {
          "name": "process_checkout",
          "description": "Process the checkout by collecting delivery address and confirming the order. Use this when the user confirms theyre ready to checkout.",
          "inputSchema": {
            "type": "object",
            "properties": {
              "store": {
                "type": "string",
                "description": "The selected store name"
              },
              "items": {
                "type": "array",
                "items": {
                  "type": "string"
                },
                "description": "List of items in the basket"
              },
              "total": {
                "type": "number",
                "description": "Total cost of the order"
              },
              "address": {
                "type": "string",
                "description": "Delivery address (if provided by user)"
              }
            },
            "required": [
              "store",
              "items",
              "total"
            ],
            "additionalProperties": false
          },
          "execute": ""
        }
      ],
      "skillId": "cc5c2c6e-8cf6-4707-80f0-1d860b974ca3",
      "version": "1.0.1"
    }
  ]
}